{
    "name": "dp/migration",
    "type": "project",
    "repositories": [{
        "type": "package",
        "package": {
            "name": "dp/migration",
            "version": "v1.0",
            "source": {
                "url": "https://github.com/rv-osaki/dp-migration.git",
                "type": "git",
                "reference": "main"
            }
        }
    }],
    "require": {
        "php": "^8.0",
        "laravel/lumen-framework": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
